  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    harmonic_alignment__input = "input.h5ad"
    harmonic_alignment__output = "output.h5ad"
    harmonic_alignment__n_svd = "100"
    harmonic_alignment__n_pca_XY = "100"
    harmonic_alignment__n_eigenvectors = "100"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
    harmonic_alignment {
      name = "harmonic_alignment"
      container = "modality_alignment/methods_harmonic_alignment"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "harmonic_alignment"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = false
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.harmonic_alignment__input}"
          dflt = "input.h5ad"
          description = "Input h5ad file containing at least `ad.X` and `ad.obsm['mode2']`."
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.harmonic_alignment__output}"
          dflt = "output.h5ad"
          description = "Output h5ad file containing both RNA and ADT data"
        }
        n_svd {
          name = "n_svd"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.harmonic_alignment__n_svd}"
          dflt = "100"
          description = "Number of SVDs to use. Bounded by the number of columns in `ad.X` and `ad.obsm['mode2']`."
        }
        n_pca_XY {
          name = "n_pca_XY"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.harmonic_alignment__n_pca_XY}"
          dflt = "100"
          description = "Default number of principal components on which to build graph."
        }
        n_eigenvectors {
          name = "n_eigenvectors"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.harmonic_alignment__n_eigenvectors}"
          dflt = "100"
          description = "Number of eigenvectors of the normalized Laplacian on which to perform alignment."
        }
      }
    }
  }