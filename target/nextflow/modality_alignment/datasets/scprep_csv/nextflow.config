  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    scprep_csv__id = "citeseq_cbmc"
    scprep_csv__input1 = "https:/www.ncbi.nlm.nih.gov/geo/download/?acc=GSE100866&format=file&file=GSE100866%5FCBMC%5F8K%5F13AB%5F10X%2DRNA%5Fumi%2Ecsv%2Egz"
    scprep_csv__input2 = "https:/www.ncbi.nlm.nih.gov/geo/download/?acc=GSE100866&format=file&file=GSE100866%5FCBMC%5F8K%5F13AB%5F10X%2DADT%5Fumi%2Ecsv%2Egz"
    scprep_csv__compression = "gzip"
    scprep_csv__test = "no_default_value_configured"
    scprep_csv__output = "viash_no_value"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py" ]
    scprep_csv {
      name = "scprep_csv"
      container = "modality_alignment/datasets_scprep_csv"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "scprep_csv"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py" ]
      }
      arguments {
        id {
          name = "id"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__id}"
          dflt = "citeseq_cbmc"
          description = "The id of the output dataset id"
        }
        input1 {
          name = "input1"
          otype = "--"
          required = false
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__input1}"
          dflt = "https:/www.ncbi.nlm.nih.gov/geo/download/?acc=GSE100866&format=file&file=GSE100866%5FCBMC%5F8K%5F13AB%5F10X%2DRNA%5Fumi%2Ecsv%2Egz"
          description = "Path or URL to the RNA counts as a gzipped csv file."
        }
        input2 {
          name = "input2"
          otype = "--"
          required = false
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__input2}"
          dflt = "https:/www.ncbi.nlm.nih.gov/geo/download/?acc=GSE100866&format=file&file=GSE100866%5FCBMC%5F8K%5F13AB%5F10X%2DADT%5Fumi%2Ecsv%2Egz"
          description = "Path or URL to the ADT counts as a gzipped csv file."
        }
        compression {
          name = "compression"
          otype = "--"
          required = false
          type = "string"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__compression}"
          dflt = "gzip"
          description = "For on-the-fly decompression of on-disk data. If 'infer' and filepath_or_buffer is path-like, then detect compression from the following extensions: '.gz', '.bz2', '.zip', or '.xz' (otherwise no decompression). If using 'zip', the ZIP file must contain only one data file to be read in. Set to None for no decompression."
        }
        test {
          name = "test"
          otype = "--"
          required = false
          type = "boolean_true"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__test}"
          description = "Subset the dataset"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.scprep_csv__output}"
          dflt = "output.h5ad"
          description = "Output h5ad file containing both input matrices data"
        }
      }
    }
  }