  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    scot__input = "input.h5ad"
    scot__output = "output.scot.h5ad"
    scot__n_svd = "100"
    scot__balanced = "no_default_value_configured"
    id = ""
    input = ""
    output = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
    scot {
      name = "scot"
      container = "modality_alignment/methods_scot"
      containerTag = "latest"
      containerRegistry = ""
      command = "scot"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = false
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scot__input}"
          dflt = "input.h5ad"
          description = "Input h5ad file containing at least `ad.X` and `ad.obsm['mode2']`."
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.scot__output}"
          dflt = "output.scot.h5ad"
          description = "Output h5ad file containing both RNA and ADT data"
        }
        n_svd {
          name = "n_svd"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scot__n_svd}"
          dflt = "100"
          description = "Number of SVDs to use. Bounded by the number of columns in `ad.X` and `ad.obsm['mode2']`."
        }
        balanced {
          name = "balanced"
          otype = "--"
          required = false
          type = "boolean_true"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.scot__balanced}"
          description = "Determines whether balanced or unbalanced optimal transport. In the balanced case, the target and source distributions are assumed to have equal mass."
        }
      }
    }
  }