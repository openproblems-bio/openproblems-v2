  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    sample_dataset__output = "viash_no_value"
    sample_dataset__n_cells = "600"
    sample_dataset__n_genes = "1500"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py" ]
    sample_dataset {
      name = "sample_dataset"
      container = "modality_alignment/datasets_sample_dataset"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "sample_dataset"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py" ]
      }
      arguments {
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.sample_dataset__output}"
          dflt = "output.h5ad"
          description = "Output h5ad file containing both input matrices data"
        }
        n_cells {
          name = "n_cells"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.sample_dataset__n_cells}"
          dflt = "600"
          description = "Number of cells"
        }
        n_genes {
          name = "n_genes"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.sample_dataset__n_genes}"
          dflt = "1500"
          description = "Number of genes"
        }
      }
    }
  }