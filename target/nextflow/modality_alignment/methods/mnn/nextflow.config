  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    mnn__input = "input.h5ad"
    mnn__output = "output.h5ad"
    mnn__n_svd = "100"
    id = ""
    testScript = "test.py"
    testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
    mnn {
      name = "mnn"
      container = "modality_alignment/methods_mnn"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "mnn"
      tests {
        isDefined = true
        testScript = "test.py"
        testResources = [ "test.py", "../../resources/sample_dataset.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = false
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.mnn__input}"
          dflt = "input.h5ad"
          description = "Input h5ad file containing at least `ad.X` and `ad.obsm['mode2']`."
        }
        output {
          name = "output"
          otype = "--"
          required = false
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.mnn__output}"
          dflt = "output.h5ad"
          description = "Output h5ad file containing both RNA and ADT data"
        }
        n_svd {
          name = "n_svd"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.mnn__n_svd}"
          dflt = "100"
          description = "Number of SVDs to use. Bounded by the number of columns in `ad.X` and `ad.obsm['mode2']`."
        }
      }
    }
  }